{
    "Serilog": {
        "Using": [
            "Serilog.Sinks.Console",
            "Serilog.Sinks.File"
        ],
        "MinimumLevel": "Debug",
        "WriteTo": [
            {
                "Name": "Console"
            },
            {
                "Name": "File",
                "Args": {
                    "path": "/var/log/feedmanager.silo.log"
                }
            }
        ],
        "Enrich": [
            "FromLogContext",
            "WithMachineName",
            "WithThreadId"
        ]
    },
    "AllowedHosts": "*",
    "REDIS_URL": "redis:6379",
    "RABBITMQ_URL": "rabbitmq",
    "RABBITMQ_VIRTUALHOST": "/",
    "RABBITMQ_USER": "rabbit",
    "RABBITMQ_PASSWORD": "rabbit"
}
